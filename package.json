{
  "name": "ve-admin-element",
  "version": "4.0.0",
  "description": "个人博客管理系统",
  "keywords": [
    "个人博客",
    "管理系统",
    "前端",
    "全栈",
    "vue3",
    "vite",
    "typescript",
    "element-plus"
  ],
  "repository": "https://github.com/ve-weiyi/ve-admin-element.git",
  "author": "791422171@qq.com",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development --host",
    "prod": "vite --mode production --host",
    "build": "vue-tsc --noEmit && vite --mode production build",
    "build:dev": "vue-tsc --noEmit && vite --mode development build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint:eslint": "eslint --cache \"src/**/*.{vue,ts}\" --fix",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,ts,json,css,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{cjs,json}": [
      "prettier --write"
    ],
    "*.{vue,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,css}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@stomp/stompjs": "^7.2.1",
    "@vueuse/core": "^13.9.0",
    "@wangeditor-next/editor": "^5.6.49",
    "@wangeditor-next/editor-for-vue": "^5.1.14",
    "animate.css": "^4.1.1",
    "axios": "1.12.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "default-passive-events": "^4.0.0",
    "echarts": "^5.6.0",
    "element-plus": "^2.11.8",
    "exceljs": "^4.4.0",
    "image-conversion": "^2.1.1",
    "lodash-es": "^4.17.21",
    "md-editor-v3": "^5.8.5",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^8.3.0",
    "pinia": "^3.0.4",
    "qs": "6.14.1",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.25",
    "vue-cropper": "^1.1.4",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.39.1",
    "@iconify/utils": "^2.3.0",
    "@types/codemirror": "^5.60.17",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.19.1",
    "@types/nprogress": "^0.2.3",
    "@types/path-browserify": "^1.0.3",
    "@types/qs": "^6.14.0",
    "@types/sortablejs": "^1.15.9",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "autoprefixer": "^10.4.22",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "sass": "^1.94.2",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "unocss": "66.3.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "7.1.11",
    "vite-plugin-mock-dev-server": "^1.9.3",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
